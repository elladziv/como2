{
  "name": "React-Base",
  "version": "1.0.1",
  "author": "Elad Ziv @Fatlady",
  "description": "React Basic App Framework",
  "dependencies": {
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "file-loader": "^0.11.2",
    "url-loader": "^0.5.8",
    "webpack": "2.4.1"
  },
  "scripts": {
    "test": "webpack --colors --progress --watch",
    "build": "webpack --colors --progress --watch --optimize-minimize"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "jsx-loader": "^0.13.2",
    "node-sass": "^4.5.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1"
  },
  "moduleNameMapper": {
    "^Styles(.*)$": "<rootDir>/src/styles$1",
    "^Components(.*)$": "<rootDir>/src/components$1",
    "^Utils(.*)$": "<rootDir>/src/utils$1",
    "^Images(.*)$": "<rootDir>/images$1",
    "^Fonts(.*)$": "<rootDir>/fonts$1",
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__mocks__/fileMock.js",
    "\\.(css|less|scss)$": "<rootDir>/client/__mocks__/styleMock.js"
  }
}
